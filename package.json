{
  "name": "stack-utils-node-internals",
  "version": "1.0.1",
  "description": "An array of node internals that can be cleaned up from stack traces",
  "repository": "tunnckoCore/stack-utils-node-internals",
  "author": "Charlike Mike Reagent <@tunnckoCore> (https://i.am.charlike.online)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard index.js test.js --fix --verbose",
    "test": "npm-run-all -s lint test:*",
    "test:api": "nyc --reporter lcov node test.js",
    "test:report": "nyc report",
    "prerelease": "npm test",
    "release": "standard-version --sign --no-verify",
    "git": "npm-run-all -s git:*",
    "git:add": "git add --all",
    "git:cz": "git-cz",
    "commit": "npm-run-all -s test git"
  },
  "dependencies": {},
  "devDependencies": {
    "commitizen": "~2.7.0",
    "cz-conventional-changelog": "1.1.5",
    "mukla": "^0.4.8",
    "npm-run-all": "~3.1.2",
    "nyc": "^10.1.2",
    "pre-commit": "^1.2.2",
    "standard": "^9.0.0",
    "standard-version": "^4.0.0"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "callsites",
    "clean",
    "cleanup",
    "help",
    "internal",
    "internals",
    "natives",
    "node",
    "node-internals",
    "regex",
    "regexp",
    "stack",
    "stacktrace",
    "traces",
    "util",
    "utils"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard": {
    "ignore": [
      "dist/**"
    ]
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "always-done",
        "minibase",
        "try-catch-core",
        "stack-utils",
        "clean-stack",
        "clean-stacktrace"
      ],
      "highlight": "stack-utils"
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "always-done",
      "async-done",
      "base",
      "charlike",
      "commitizen",
      "dezalgo",
      "once",
      "standard-version",
      "verb",
      "verb-generate-readme"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "lines": 100
  }
}
